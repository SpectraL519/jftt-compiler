# Simple + operator test
PROGRAM IS
    a, b, sum, diff, prod, quot, mod
IN
    READ a;
    READ b;

    sum := a + b;
    WRITE sum;

    diff := a - b;
    WRITE diff;
    diff := b - a;
    WRITE diff;

    prod := a * b;
    WRITE prod;

    quot := a / b;
    WRITE quot;
    quot := b / a;
    WRITE quot;
END
